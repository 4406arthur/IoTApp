
<div class="row">
    <aside class="col-md-3">
      <section>
        <%= render 'shared/user_info' %>
      </section>
      <section>
        <%= render 'shared/menu' %>
      </section>
    </aside>
    <div class="col-md-8">
      <h3>Environment data info</h3>
       <%= bootstrap_form_tag url:'/chart' do |f| %>
          <%= f.date_field :select %>
          <%= f.submit "Search", class: "btn btn-success"%>
        <% end %>
      <div class="center lead">
        <% @devices.each do |device| %>
          <% if device.category != '60' && device.category != '61' %>
            <em><%= device.name %></em>
            <%= line_chart device.sense_values.group_by_day(:created_at).average(:data), library: {backgroundColor: "#eee"} %>
          <% end %>
        <%end%>
      </div>
    </div>
</div>
