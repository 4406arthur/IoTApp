<%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>
<div class="row">
    <aside class="col-md-4">
      <section>
        <%= render 'shared/user_info' %>
      </section>
      <section>
        <%= render 'shared/menu' %>
      </section>
    </aside>
    <div class="col-md-8">
      <h3>Environment data info</h3>
      <div class="center lead">
      <%= line_chart @devices.group(:name).group_by_second(:created_at).sum(:device_id) %>
      </div>
    </div>
</div>