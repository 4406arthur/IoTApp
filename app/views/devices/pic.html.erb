<div class="row">
    <aside class="col-md-3">
      <section>
        <%= render 'shared/user_info' %>
      </section>
      <section>
        <%= render 'shared/menu' %>
      </section>
    </aside>
    <div class="col-md-8">
      <h3>Ipcam Capture</h3>
      <div class="center lead">
        <div id="gallery" data-toggle="modal-gallery" data-target="#modal-gallery">
        <% @cams.each do |cam| %>
          <% cam.sense_values.each do |image| %>
            <%= image_tag(image.description, :data => { :gallery => "gallery" }, :class => "img-thumbnail" ) %>
            <% end %>
        <% end %>
        </div>
      </div>
    </div>
</div>
